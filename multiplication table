#include <iostream>
using namespace std;
class table{
    
    public:
    //void set(int b){
       // b=a;
        
    //}
    int b;
    void set(){
        cin>>b;
    }
    void display(){
        for(int i=1;i<=10;i++){
            cout<<b<<" x "<<i<<" = "<<(b*i)<<endl;
        }
    }
    
};
int main(){
    table abhi;
    //abhi.set(2);
    abhi.set();
    abhi.display();
    return 0;
}
