#include<iostream>
using namespace std;
int fact(int a){
    long double factorial = 1.0;
    if(a<=1){
        cout<<1;
    }else{
        for(int i=1;i<=a;i++){
            factorial *=i;
        }
        cout<<factorial;
    }
 return 0;   
}
int main(){
    int n;
    cin>>n;
    fact(n);
    return 0;
}
